<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mju.groupware.dao.TeamDao">
	<select id="SelectClassName" resultType="String">

		Select ClassName from
		Class;
	</select>

	<select id="SelectProfessorName" parameterType="String"
		resultType="String">
		Select ClassProfessorName from Class where ClassName = #{ClassName};
	</select>


</mapper>